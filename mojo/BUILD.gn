import("//mojo/public/tools/bindings/mojom.gni")

mojom("url_mojom") {
  sources = [
    "url.mojom",
  ]
}

mojom("url_mojom_gurl") {
  sources = [
    "url.mojom",
  ]

  variant = "chromium"
  typemaps = [ "gurl.typemap" ]

  public_deps = [
    ":url_mojom",
  ]
}

mojom("test_url_mojom_gurl") {
  sources = [
    "url_test.mojom",
  ]

  typemaps = [ "gurl.typemap" ]

  public_deps = [
    ":url_mojom_gurl",
  ]
}
